<h1>Contactez nous!</h1>
<br>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<form>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Adresse mail*</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="exemple: adresse@mail.com" required>
    <div id="emailHelp" class="form-text">
    </div>
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">N° de téléphone*</label>
    <input type="number" class="form-control" id="exampleInputTel1" maxlength="14">
  </div>

  <div class="form-floating">
  <textarea class="textarea" 
  rows = "5" cols = "100"
   id="floatingTextarea" placeholder="Ecrivez votre message"></textarea>
  <label for="floatingTextarea"></label>
  
</div>
  <div class="mb-3 form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1" required>
    <label class="form-check-label" for="exampleCheck1">Valider</label>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
